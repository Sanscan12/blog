<span class="token comment">// Main类调用 该实例</span>
<span class="token keyword">public</span> <span class="token keyword">class</span> <span class="token class-name">WhirlTest</span> <span class="token keyword">extends</span> <span class="token class-name">Canvas</span> <span class="token punctuation">{</span>

    <span class="token keyword">int</span> <span class="token class-name">W</span> <span class="token punctuation">,</span> <span class="token class-name">H</span> <span class="token punctuation">;</span>
    <span class="token class-name">Image</span> image<span class="token punctuation">;</span>
